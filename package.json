{
  "name": "niclas.tech",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "serve": "vite",
    "build": "vue-tsc -b && vite build",
    "deploy": "aws s3 sync --delete --exclude \".*\" dist/ s3://niclas.tech --acl public-read --cache-control max-age=3600",
    "invalidate-cache": "aws cloudfront create-invalidation --distribution-id E2M7XPIQQZ78UE --paths /"
  },
  "dependencies": {
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@aws-solutions-constructs/aws-cloudfront-s3": "^2.46.0",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/compiler-sfc": "^3.3.8",
    "aws-cdk-lib": "^2.110.1",
    "constructs": "^10.3.0",
    "ts-node": "^10.9.1",
    "typescript": "5.6.2",
    "vite": "^5.0.2",
    "vue-tsc": "2.0.29"
  }
}
